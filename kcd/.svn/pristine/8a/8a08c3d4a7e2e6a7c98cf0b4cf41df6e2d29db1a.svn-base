.allshop cover-image {
  width: 30rpx;
  height: 30rpx;
  margin-top: 15rpx;
  margin-left: 5rpx;
  float: left;
}

.mendian {
  float: left;
  margin-left: 35%;
  font-size: 28rpx;
  line-height: 60rpx;
  height: 60rpx;
  /* line-height: 60rpx; */
}

.test {
  /* margin:8rpx 20rpx 10rpx 20rpx; *//* height:260rpx; *//* width:100%;  */
  background: #fff;
  display: flex;
  margin-bottom: 20rpx;
  height:240rpx;
}

.section_img {
  width: 30%;
  padding: 15rpx;
  padding-top: 10rpx;
}

.section_img image {
  width: 100%;
  height: 100%;
}

.session_content {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.q-top {
  flex: 1;
  display: flex;
  padding-top: 10rpx;
  margin-right: 5rpx;
  margin-left: 5rpx;
  border-bottom: 1px solid #eee;
}

.left {
  flex: 1;
}

.left .left_tit {
  font-size: 30rpx;
  /* line-height: 60rpx; */
}

.left .left_tit1 {
  font-size: 24rpx;
  color: #999;
  line-height: 40rpx;
}

.right {
  width: 180rpx;
}

.right .right_top {
  font-size: 26rpx;
  padding-left: 10rpx;
  text-align: right;
}

.right .right_top image {
  width: 35rpx;
  height: 35rpx;
}

.right .right_top cover-image {
  width: 35rpx;
  height: 35rpx;
  display: block;
  line-height: 1.2;
  float: right;
}

.mdxq {
  float: left;
  margin-left: 30rpx;
}

.right_distance {
  font-size: 24rpx;
  color: #999;
  text-align: right;
  padding-right:8rpx;
}

.q-bot {
  flex: 1;
}

.q_bot_q {
  float: left;
  width: auto;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  margin-left: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  background: rgb(238, 158, 10);
  padding: 3rpx;
  border-radius: 5rpx;
}

.show {
  width: 100%;
  z-index: 553334444444444444333335;
  position: absolute;
  left: 0;
  bottom: 80rpx;
  /* top:80rpx; */
  background: #fff;
  overflow-y: hidden;
  height: 310rpx;
  overflow: hidden;
}

.hide {
  width: 100%;
  z-index: 553334444444444444333335;
  position: absolute;
  left: 0;
  bottom: 80rpx;
  top: 80rpx;
  background: #fff;
  overflow-y: hidden;
}

.scr {
  background: #eee;
}

.scr1 {
  background: #eee;
  overflow-y: scroll;
}

cover-view {
  display: block;
  line-height: 1.2;
  overflow: hidden;
  white-space: normal;
  pointer-events: auto;
}

.dialog-longline {
  width: 100%;
  height: 2rpx;
  background-color: #ccc;
}
