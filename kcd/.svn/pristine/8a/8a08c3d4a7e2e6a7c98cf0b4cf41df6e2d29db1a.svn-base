page {
  height: 100%;
}

.contaier {
  width: 100%;
  height: 100%;
  position: relative;
}

.address {
  display: flex;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
  flex-direction: row;
}

.city {
  width: 100rpx;
  font-size: 28rpx;
  text-indent: 5rpx;
}

.add_input {
  flex: 1;
}

.add_input_body {
  margin-top: 15rpx;
  margin-left: 20rpx;
  margin-right: 60rpx;
  height: 50rpx;
  background: #eee;
  border-radius: 5rpx;
}

.add_input .add_input_body image {
  width: 48rpx;
  height: 48rpx;
  float: left;
  vertical-align: center;
  margin-top: 1rpx;
  margin-bottom: 2rpx;
}

.add_input .add_input_body input {
  float: left;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  text-indent: 20rpx;
  margin-left: 5rpx;
}

/* ditu */

.map {
  position: absolute;
  left: 0;
  right: 0;
  top: 80rpx;
  bottom: 80rpx;
  background: #ccc;
}

.order {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  background: #000;
  opacity: 0.8;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}

